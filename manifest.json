{
    "manifest_version": 3,
    "name" : "gruppy",
    "description" : "shortcuts for tab groups",
    "version": "0.0.1",
    "permissions": ["tabGroups"],
    "action" : {},
    "background": {
        "service_worker": "gruppy.js"
    },
    "commands": {
      "gruppy_toggle": {
        "suggested_key": {
          "default": "Alt+G"
        },
        "description": "Toggle grouping"
      },
      "gruppy_close": {
        "suggested_key": {
          "default": "Alt+Shift+G"
        },
        "description": "Close Group"
      }
    }
}



